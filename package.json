{
  "name": "schwepe-analyzer",
  "version": "1.2.0",
  "description": "AI-powered media content analyzer for detecting Schwepe meme tokens in images and videos",
  "main": "balanced-image-schwepe-analyzer.js",
  "scripts": {
    "download": "node discord-media-downloader.js",
    "download:24h": "node discord-media-downloader.js 24",
    "download:12h": "node discord-media-downloader.js 12",
    "download:6h": "node discord-media-downloader.js 6",
    "download:2h": "node discord-media-downloader.js 2",
    "download:1h": "node discord-media-downloader.js 1",
    "analyze:images": "node balanced-image-schwepe-analyzer.js",
    "analyze:videos": "node balanced-video-schwepe-analyzer.js",
    "analyze:all": "node balanced-image-schwepe-analyzer.js & node balanced-video-schwepe-analyzer.js",
    "test:false-positives": "node test-false-positives.js",
    "schedule:start": "node video-scheduler.js start",
    "schedule:info": "node video-scheduler.js info",
    "schedule:regenerate": "node video-scheduler.js regenerate",
    "schedule:test-static": "node video-scheduler.js test-static",
    "schedule:build": "BUILD_ENV=true node video-scheduler.js regenerate"
  },
  "keywords": [
    "ai",
    "content-analysis",
    "meme-detection",
    "schwepe",
    "media-analyzer",
    "image-analysis",
    "video-analysis",
    "machine-learning",
    "classification",
    "meme-token",
    "crypto"
  ],
  "author": "Schwepe Community",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "discord.js": "^14.22.1",
    "dotenv": "^16.4.7",
    "fs-extra": "^11.0.0"
  },
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/schwepe-community/schwepe-analyzer.git"
  },
  "bugs": {
    "url": "https://github.com/schwepe-community/schwepe-analyzer/issues"
  },
  "homepage": "https://github.com/schwepe-community/schwepe-analyzer#readme"
}
